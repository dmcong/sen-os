    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm:universe\" \"npm:dev\"",
    "dev": "nodemon --watch ./src/static/styles/theme.js --exec \"REACT_APP_ENV=development PORT=5000 craco start\"",
    "stag": "REACT_APP_ENV=staging npm run build",
    "prod": "REACT_APP_ENV=production npm run build",
    "prebuild": "node ./scripts/packageWatcher.js",
    "build": "craco build",
    "serve": "serve -s build -l 80",
    "universe": "node ./scripts/packageWatcher.js",
    "analyze": "npm run prod && source-map-explorer 'build/static/js/*.js'",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.0",
    "craco-alias": "^3.0.1",
    "craco-less": "^1.18.0",
    "nodemon": "^2.0.7",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2"
  }
}
