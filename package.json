    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "flexsearch": "^0.7.2",
    "js-file-download": "^0.4.12",
    "localforage": "^1.9.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.6.2",
    "react": "^17.0.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
