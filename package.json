  "repository": {
    "type": "git",
    "url": "https://github.com/DescartesNetwork/sen-os.git"
  },
  "dependencies": {
    "@dnd-kit/core": "^3.1.1",
    "@dnd-kit/sortable": "^4.0.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@senswap/sen-js": "0.0.16",
    "@senswap/sen-ui": "0.0.1",
    "@solana/spl-token-registry": "^0.2.203",
    "axios": "^0.21.1",
    "flexsearch": "^0.7.2",
    "ipfs-core": "^0.8.0",
    "js-file-download": "^0.4.12",
    "localforage": "^1.9.0",
    "numeral": "^2.0.6",
    "randomcolor": "^0.6.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-json-view": "^1.21.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "remarkable": "^2.0.1",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm:prebuild\" \"npm:dev\"",
    "dev": "REACT_APP_ENV=development PORT=5000 craco start",
    "stag": "REACT_APP_ENV=staging npm run build",
    "prod": "REACT_APP_ENV=production npm run build",
    "prebuild": "node ./scripts/packageWatcher.js",
    "build": "craco build",
    "serve": "serve -s build -l 80",
    "analyze": "npm run prod && source-map-explorer 'build/static/js/*.js'",
    "postinstall": "find ./src/applications/* -name package.json -maxdepth 1 -execdir npm install \\;",
    "new": "node ./scripts/templateGenerator.js",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.0",
    "craco-alias": "^3.0.1",
    "craco-less": "^1.18.0",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.7",
    "prompts": "^2.4.1",
    "replace-in-file": "^6.2.0",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2"
  }
}
